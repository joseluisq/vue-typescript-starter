{
  "compilerOptions": {
    "outDir": "./built/",
    "sourceMap": true,
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "types": ["vue-typescript-import-dts"],
    "lib": ["dom", "es5", "es2015.promise", "es2015.iterable"]
  },
  "include": ["./src/**/*"]
}
